<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="561.0" prefWidth="1182.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.ufp.inf.javafx.home.MainController">
    <children>
        <MenuBar layoutX="-1.0" layoutY="6.0" prefHeight="107.0" prefWidth="973.0">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <Menu mnemonicParsing="false" text="Open">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Open Text File" />
                                <MenuItem mnemonicParsing="false" text="Open Bin File" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Save">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Save Text File" />
                                <MenuItem mnemonicParsing="false" onAction="#handleSaveBinFileAction" text="Save Bin File" />
                            </items>
                        </Menu>
                        <MenuItem mnemonicParsing="false" text="Exit" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <TabPane layoutX="-1.0" layoutY="29.0" prefHeight="532.0" prefWidth="1182.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="Authors">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="503.0" prefWidth="1223.0">
                            <children>
                                <VBox layoutY="6.0" prefHeight="498.0" prefWidth="1119.0">
                                    <children>
                              <StackPane prefHeight="387.0" prefWidth="1166.0">
                                 <children>
                                              <TableView fx:id="authorsTable" editable="true" prefHeight="376.0" prefWidth="1146.0">
                                                  <columns>
                                                      <TableColumn fx:id="idNumberCol" onEditCommit="#handleEditVehicleAction" prefWidth="70.0" text="idNumber" />
                                                      <TableColumn fx:id="birthDateCol" onEditCommit="#handleEditVehicleAction" prefWidth="130.0" text="birthDate" />
                                                      <TableColumn fx:id="nameCol" onEditCommit="#handleEditVehicleAction" prefWidth="174.0" text="Name" />
                                                      <TableColumn fx:id="addressCol" onEditCommit="#handleEditVehicleAction" prefWidth="100.0" text="Address" />
                                          <TableColumn fx:id="penNameCol" onEditCommit="#handleEditVehicleAction" prefWidth="100.0" text="penName" />
                                          <TableColumn fx:id="affiliationCol" onEditCommit="#handleEditVehicleAction" prefWidth="100.0" text="affiliation" />
                                          <TableColumn fx:id="orcIDCol" onEditCommit="#handleEditVehicleAction" prefWidth="165.0" text="orcID" />
                                          <TableColumn fx:id="scienceIDCol" prefWidth="75.0" text="scienceID" />
                                          <TableColumn fx:id="googleScholarIDCol" prefWidth="75.0" text="googleScholarID" />
                                          <TableColumn fx:id="scopusAuthorIDCol" prefWidth="75.0" text="scopusAuthorID" />
                                                  </columns>
                                              </TableView>
                                 </children>
                              </StackPane>
                                        <HBox prefHeight="101.0" prefWidth="1148.0" spacing="5.0">
                                            <VBox.margin>
                                                <Insets />
                                            </VBox.margin>
                                            <padding>
                                                <Insets left="5.0" top="5.0" />
                                            </padding>
                                            <children>
                                                <TextField fx:id="registrationField" prefHeight="27.0" prefWidth="87.0" promptText="birthDate" />
                                                <TextField fx:id="brandField" prefHeight="27.0" prefWidth="119.0" promptText="Name" />
                                                <TextField fx:id="modelField" prefHeight="27.0" prefWidth="125.0" promptText="Address" />
                                                <TextField fx:id="cylindersField" prefHeight="27.0" prefWidth="124.0" promptText="penName" />
                                    <TextField prefHeight="25.0" prefWidth="108.0" promptText="affiliation" />
                                    <TextField prefHeight="25.0" prefWidth="123.0" promptText="orcID" />
                                    <TextField prefHeight="25.0" prefWidth="118.0" promptText="scienceID" />
                                    <TextField prefHeight="25.0" prefWidth="116.0" promptText="googleScholarID" />
                                                <Button mnemonicParsing="false" onAction="#handleAddVehicleAction" prefHeight="25.0" prefWidth="53.0" text="Add">
                                                    <HBox.margin>
                                                        <Insets left="10.0" />
                                                    </HBox.margin>
                                                </Button>
                                            </children>
                                        </HBox>
                              <ScrollBar />
                                    </children>
                                </VBox>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Papers">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                </Tab>
                <Tab text="Penalties">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <VBox prefHeight="339.0" prefWidth="600.0">
                                    <children>
                                        <HBox prefHeight="40.0" prefWidth="600.0" spacing="10.0">
                                            <children>
                                                <ComboBox fx:id="vehicleComboBox" onAction="#handleSelectVehicleAction" prefHeight="27.0" prefWidth="295.0" promptText="Vehicle" />
                                                <ComboBox fx:id="driversComboBox" onAction="#handleSelectDriverAction" prefHeight="27.0" prefWidth="291.0" promptText="Driver" />
                                            </children>
                                            <padding>
                                                <Insets left="5.0" right="5.0" top="5.0" />
                                            </padding>
                                            <VBox.margin>
                                                <Insets bottom="5.0" />
                                            </VBox.margin>
                                        </HBox>
                                        <TableView fx:id="penaltiesTable" prefHeight="315.0" prefWidth="600.0">
                                            <columns>
                                                <TableColumn fx:id="driverCol" prefWidth="87.0" text="Driver" />
                                                <TableColumn fx:id="vehicleCol" prefWidth="136.0" text="Vehicle" />
                                                <TableColumn fx:id="dateCol" prefWidth="120.0" text="Date" />
                                                <TableColumn fx:id="localCol" prefWidth="118.0" text="Local" />
                                                <TableColumn fx:id="motiveCol" prefWidth="138.0" text="Motive" />
                                            </columns>
                                        </TableView>
                                    </children>
                                </VBox>
                            </children></AnchorPane>
                    </content>
                </Tab>
            <Tab text="Untitled Tab">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
